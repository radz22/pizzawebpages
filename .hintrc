{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-charset-utf-8": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "@-ms-viewport"
        ]
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 111",
    "not and_ff <= 110",
    "not chrome <= 111",
    "not edge <= 111",
    "not firefox <= 111",
    "not ios_saf <= 16.3",
    "not opera <= 95",
    "not safari <= 16.3",
    "not samsung <= 20"
  ]
}